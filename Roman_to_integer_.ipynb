      "cell_type": "code",
      "source": [
        "a={'I':1,'V':5,'X':10,'L':50,'C':100,'D':500,'M':1000}\n",
        "x=input(\"enter a roman number\")\n",
        "sum=0\n",
        "for i in range(len(x)-1,0,-1):\n",
        "    if a[x[i-1]]>=a[x[i]]:\n",
        "        c=x[i-1]\n",
        "        sum=sum+a[c]\n",
        "    else:\n",
        "        c=x[i-1]\n",
        "        sum=sum-a[c]\n",
        "sum=sum + a[x[len(x)-1]]\n",
        "print(sum)"
      ],
